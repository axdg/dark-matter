{
  "name": "dark-matter",
  "version": "0.0.1",
  "repository": "https://github.com/axdg/dark-matter.git",
  "description": "Post front-matter and body extractor / parser",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require blanket -R mocha-lcov-reporter | ./node_modules/coveralls/bin/coveralls.js"
  },
  "author": "axdg <axdg@dfant.asia>",
  "license": "MIT",
  "devDependencies": {
    "benchmark": "^1.0.0",
    "blanket": "^1.1.7",
    "coveralls": "^2.11.4",
    "front-matter": "^1.0.0",
    "gray-matter": "^2.0.1",
    "mocha-lcov-reporter": "0.0.2"
  },
  "dependencies": {
    "js-yaml": "^3.3.1"
  },
  "config": {
    "blanket": {
      "pattern": "index.js",
      "data-cover-never": [
        "node_modules",
        "test.js"
